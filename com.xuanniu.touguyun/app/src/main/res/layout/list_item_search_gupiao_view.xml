<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="72dp">

    <TextView
        android:id="@id/item_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16.7dp"
        android:layout_marginLeft="23dp"
        android:textColor="@color/black_323232"
        android:textSize="@dimen/sp_16"
        android:text="同花顺"
        />
    <TextView
        android:id="@id/item_profit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="39dp"
        android:layout_marginLeft="23dp"
        android:textColor="@color/black_323232"
        android:textSize="@dimen/sp_16"
        android:text="(SZ300033)"
        />
    <com.touguyun.view.FilletRelativeLayout
        android:id="@id/item_add"
        android:layout_width="40dp"
        android:layout_height="26dp"
        app:defaultBackColor="@color/blue_38AAFF"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="23dp"
        android:visibility="gone"
        >
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:src="@drawable/add_white_icon"
            android:padding="5dp"
            />
        </com.touguyun.view.FilletRelativeLayout>
    <com.touguyun.view.FilletRelativeLayout
        android:id="@id/item_cancel"
        android:layout_width="40dp"
        android:layout_height="26dp"
        app:defaultBackColor="@color/gray_c8c8c8"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="23dp"
        android:visibility="gone"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_13"
            android:text="取消"
            />
        </com.touguyun.view.FilletRelativeLayout>
    <TextView
        android:id="@id/item_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="23dp"
        android:textSize="@dimen/sp_13"
        android:textColor="@color/gray_969696"
        android:text="已添加"
        />
</RelativeLayout>